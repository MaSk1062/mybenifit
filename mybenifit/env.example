# Firebase Configuration
# Copy this file to .env and fill in your Firebase project details

# API Key from Firebase Console
VITE_FIREBASE_API_KEY=your_api_key_here

# Auth Domain (usually your-project-id.firebaseapp.com)
VITE_FIREBASE_AUTH_DOMAIN=mybenifit-app.firebaseapp.com

# Project ID
VITE_FIREBASE_PROJECT_ID=mybenifit-app

# Storage Bucket (usually your-project-id.appspot.com)
VITE_FIREBASE_STORAGE_BUCKET=mybenifit-app.appspot.com

# Messaging Sender ID
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here

# App ID
VITE_FIREBASE_APP_ID=your_app_id_here

# Measurement ID (optional, for Analytics)
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Development Environment
NODE_ENV=development

# Build Configuration
VITE_APP_TITLE=MyBenefit
VITE_APP_VERSION=1.0.0 